[run]
source = peeemtee

[report]
include =
    peeemtee/*
omit =
    peeemtee/tests/*
    peeemtee/pipe_modules.py
exclude_lines =
    pragma: no cover
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if self.debug:
    if settings.DEBUG
    def __repr__
